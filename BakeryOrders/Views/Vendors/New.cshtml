@{
  Layout = "_Layout";
} 
<h1>Add a vendor</h1>

<form action="/vendors" method="post">
  <label for="vendorName">vendor name:</label><br>
  <input id="vendorName" name="vendorName" type="text"><br>
  <label for="vendorDescription">vendor description:</label><br>
  <input id="vendorDescription" name="vendorDescription" type="text"><br>
  <button type="submit">Add</button>
</form>